else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/Moonshine/Desktop/学习/前端/node/koa-ts/node_modules/.pnpm/registry.npmmirror.com+yamljs@0.3.0/node_modules/yamljs/bin/node_modules:/mnt/c/Users/Moonshine/Desktop/学习/前端/node/koa-ts/node_modules/.pnpm/registry.npmmirror.com+yamljs@0.3.0/node_modules/yamljs/node_modules:/mnt/c/Users/Moonshine/Desktop/学习/前端/node/koa-ts/node_modules/.pnpm/registry.npmmirror.com+yamljs@0.3.0/node_modules:/mnt/c/Users/Moonshine/Desktop/学习/前端/node/koa-ts/node_modules/.pnpm/node_modules:/mnt/c/Users/Moonshine/Desktop/学习/前端/node/koa-ts/node_modules:/mnt/c/Users/Moonshine/Desktop/学习/前端/node/node_modules:/mnt/c/Users/Moonshine/Desktop/学习/前端/node_modules:/mnt/c/Users/Moonshine/Desktop/学习/node_modules:/mnt/c/Users/Moonshine/Desktop/node_modules:/mnt/c/Users/Moonshine/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Moonshine/Desktop/学习/前端/node/koa-ts/node_modules/.pnpm/registry.npmmirror.com+pm2@5.2.0/node_modules/pm2/node_modules:/mnt/c/Users/Moonshine/Desktop/学习/前端/node/koa-ts/node_modules/.pnpm/registry.npmmirror.com+pm2@5.2.0/node_modules"
