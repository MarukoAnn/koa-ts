#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Moonshine/Desktop/学习/前端/node/koa-ts/node_modules/.pnpm/registry.npmmirror.com+ts-node@10.7.0/node_modules/ts-node/dist/node_modules:/mnt/c/Users/Moonshine/Desktop/学习/前端/node/koa-ts/node_modules/.pnpm/registry.npmmirror.com+ts-node@10.7.0/node_modules/ts-node/node_modules:/mnt/c/Users/Moonshine/Desktop/学习/前端/node/koa-ts/node_modules/.pnpm/registry.npmmirror.com+ts-node@10.7.0/node_modules:/mnt/c/Users/Moonshine/Desktop/学习/前端/node/koa-ts/node_modules/.pnpm/node_modules:/mnt/c/Users/Moonshine/Desktop/学习/前端/node/koa-ts/node_modules:/mnt/c/Users/Moonshine/Desktop/学习/前端/node/node_modules:/mnt/c/Users/Moonshine/Desktop/学习/前端/node_modules:/mnt/c/Users/Moonshine/Desktop/学习/node_modules:/mnt/c/Users/Moonshine/Desktop/node_modules:/mnt/c/Users/Moonshine/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/Moonshine/Desktop/学习/前端/node/koa-ts/node_modules/.pnpm/registry.npmmirror.com+ts-node@10.7.0/node_modules/ts-node/dist/node_modules:/mnt/c/Users/Moonshine/Desktop/学习/前端/node/koa-ts/node_modules/.pnpm/registry.npmmirror.com+ts-node@10.7.0/node_modules/ts-node/node_modules:/mnt/c/Users/Moonshine/Desktop/学习/前端/node/koa-ts/node_modules/.pnpm/registry.npmmirror.com+ts-node@10.7.0/node_modules:/mnt/c/Users/Moonshine/Desktop/学习/前端/node/koa-ts/node_modules/.pnpm/node_modules:/mnt/c/Users/Moonshine/Desktop/学习/前端/node/koa-ts/node_modules:/mnt/c/Users/Moonshine/Desktop/学习/前端/node/node_modules:/mnt/c/Users/Moonshine/Desktop/学习/前端/node_modules:/mnt/c/Users/Moonshine/Desktop/学习/node_modules:/mnt/c/Users/Moonshine/Desktop/node_modules:/mnt/c/Users/Moonshine/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/bin-esm.js" "$@"
else
  exec node  "$basedir/../../dist/bin-esm.js" "$@"
fi
